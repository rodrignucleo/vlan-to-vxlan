	publisher = {{IEEE}},
	author = {Li, Xinqiao and Liu, Mingyuan and Cheng, Nan and Liu, Kang and Quan, Wei and Guo, Liang and Qin, Yajuan},
	date = {2022},
	note = {{ISSN}: 2325-5595
Num Pages: 6
Series Title: {IEEE} International Conference on High Performance Switching and Routing
Web of Science {ID}: {WOS}:000855525500009},
	keywords = {delay-sensitive, programmable data planes, {QoS}, queue scheduling},
	file = {Full Text PDF:files/152/Li et al. - 2022 - RPQ Resilient-Priority Queue Scheduling for Delay-Sensitive Applications.pdf:application/pdf},
